{
  "name": "infographic-for-react",
  "version": "0.3.0",
  "description": "React components for @antv/infographic - a declarative, component-based wrapper for infographic generation",
  "keywords": [
    "antv",
    "infographic",
    "react",
    "visualization",
    "infographic-for-react"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyw405/infographic-for-react.git"
  },
  "bugs": {
    "url": "https://github.com/lyw405/infographic-for-react/issues"
  },
  "homepage": "https://github.com/lyw405/infographic-for-react#readme",
  "license": "MIT",
  "author": "AntV",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "test": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "ci": "run-s typecheck lint test",
    "prepublishOnly": "npm run typecheck && npm run test"
  },
  "peerDependencies": {
    "@antv/infographic": "^0.2.7",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@antv/infographic": "^0.2.7",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "happy-dom": "^16.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "tsup": "^8.4.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "lodash-es": "^4.17.21"
  }
}
